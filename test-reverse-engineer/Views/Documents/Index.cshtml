@model IEnumerable<test_reverse_engineer.Models.Document>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DocId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocStream)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocCreatedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocModifiedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Project)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DocId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocStream)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocCreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocModifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category.CategoryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Project.ProjectId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DocId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DocId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DocId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
